let a = 5;
let b = '5';
let c = 10;
console.log(a == b); // true, because of type coercion
console.log(a === b); // false, because they are of different types
console.log(a != b); // false, because of type coercion
console.log(a !== b); // true, because they are of different types
console.log(a < c); // true, because 5 is less than 10
console.log(a > c); // false, because 5 is not greater than 10
console.log(a <= 5); // true, because 5 is equal to 5
console.log(a >= 10); // false, because 5 is less than 10
